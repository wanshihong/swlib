{
  "name": "wansh/swlib",
  "description": "Swlib库 - 基于Swoole的PHP简易框架",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "App/",
      "Generate\\": "runtime/Generate",
      "Swlib\\": "Swlib/src",
      "GPBMetadata\\": "runtime/Protobuf/GPBMetadata/",
      "Protobuf\\": "runtime/Protobuf/Protobuf/"
    }
  },
  "require": {
    "php": ">=8.4",
    "ext-swoole": ">=6.0.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-redis": "*",
    "ext-bcmath": "*",
    "ext-curl": "*",
    "ext-openssl": "*",
    "ext-mysqli": "*",
    "ext-mbstring": "*",
    "twig/twig": "^3.14",
    "ext-zlib": "*",
    "ext-gd": "*",
    "guzzlehttp/guzzle": "^7.9"
  },
  "require-dev": {
    "swoole/ide-helper": "*"
  },
  "authors": [
    {
      "name": "wansh",
      "email": "wsh255@qq.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true
}
